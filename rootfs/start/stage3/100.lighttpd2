# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   if [ ! -s "$VAR_CONFIG_DIR/angel.conf" ]
   then
      createAngelConf
   fi
   if [ ! -s "$VAR_CONFIG_DIR/lighttpd.conf" ]
   then
      createLighttpdConf
   fi
   if [ ! -s "$VAR_CONFIG_DIR/mimetypes.conf" ]
   then
      zcat /mimetypes.conf.gz > "$VAR_CONFIG_DIR/mimetypes.conf"
   fi
fi
